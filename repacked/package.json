{
  "name": "repacked",
  "version": "1.1.6",
  "main": "index.js",
  "repository": "git@github.com:rukshan-dev/repacked.git",
  "author": "Rukshan Fernando<hello@rukshan.dev>",
  "keywords": [
    "framework",
    "react",
    "express",
    "jest",
    "create-react",
    "scripts",
    "typescript",
    "javascript",
    "app",
    "frontend",
    "server",
    "rspack"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build:npm": "tsup && node ./scripts/generateManifest.js generate --registry=npm && cp LICENSE ./dist/LICENSE && cp ../README.md ./dist/README.md",
    "build:github": "tsup && node ./scripts/generateManifest.js generate --registry=github && node ./scripts/setupNpmrc.js && cp LICENSE ./dist/LICENSE",
    "repacked": "yarn build && node ./dist/cli.js",
    "deploy": "yarn --cwd ./dist npm publish"
  },
  "dependencies": {
    "@module-federation/enhanced": "0.6.15",
    "@rspack/cli": "1.3.15",
    "@rspack/core": "1.3.15",
    "@rspack/dev-server": "1.1.3",
    "@rspack/plugin-react-refresh": "1.4.3",
    "@swc/core": "1.7.42",
    "@swc/jest": "0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.14",
    "chalk": "4.1.2",
    "connect-history-api-fallback": "2.0.0",
    "css-loader": "7.1.2",
    "dotenv": "16.4.5",
    "file-loader": "6.2.0",
    "http-proxy-middleware": "3.0.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "license-webpack-plugin": "4.0.2",
    "open": "10.1.0",
    "react-refresh": "0.14.2",
    "style-loader": "4.0.0",
    "ts-loader": "^9.5.1",
    "tsup": "8.3.5",
    "typescript": "^5.6.3",
    "webpack-dev-middleware": "7.4.2",
    "webpack-hot-middleware": "2.26.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "1.5.4",
    "@types/express": "5.0.1",
    "@types/node": "22.8.6",
    "@types/webpack-hot-middleware": "2.25.9",
    "@types/yargs": "17.0.33"
  }
}
