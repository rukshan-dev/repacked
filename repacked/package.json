{
  "name": "repacked",
  "version": "0.0.3-alpha",
  "main": "index.js",
  "repository": "git@github.com:rukshan-dev/repacked.git",
  "author": "Rukshan Fernando<hello@rukshan.dev>",
  "license": "MIT",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build:npm": "tsup && node ./scripts/generateManifest.js generate --registry=npm && cp LICENSE ./dist/LICENSE && cp ../README.md ./dist/README.md",
    "build:github": "tsup && node ./scripts/generateManifest.js generate --registry=github && node ./scripts/setupNpmrc.js && cp LICENSE ./dist/LICENSE",
    "repacked": "yarn build && node ./dist/cli.js",
    "deploy": "yarn --cwd ./dist npm publish"
  },
  "dependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@module-federation/enhanced": "0.6.15",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.14",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "dotenv": "16.4.5",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "react-refresh": "0.14.2",
    "style-loader": "4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "5.96.1",
    "webpack-dev-server": "5.1.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@swc/core": "1.7.42",
    "@types/node": "22.8.6",
    "@types/yargs": "17.0.33",
    "tsup": "8.3.5"
  }
}
